  <component name="FindInProjectRecents">
    <findStrings>
      <find>from preparation\.init import app\, db\nfrom models\.info import info\, info_schema\nfrom flask import request\, jsonify\n\nifs \= info_schema\(\)\nifss \= info_schema\(many \= True\)\n\n\@app\.route\(\&quot;\/info\&quot;\, methods\=\[\&quot;GET\&quot;\]\)\ndef get_all_info\(\)\:\n    all_info \= info\.query\.all\(\)\n    result \= ifss\.dump\(all_info\)\n    return jsonify\(result\.data\)\n\n\@app\.route\(\&quot;\/shirt\&quot;\, methods\=\[\&quot;POST\&quot;\]\)\ndef add_shirt\(\)\:\n    try\:\n        shirt_name \= request\.form\.get\(\'shirt_name\'\)\n        material_name \= request\.form\.get\(\'material_name\'\)\n        producer_name \= request\.form\.get\(\'producer_name\'\)\n        price \= request\.form\.get\(\'price\'\)\n        size_avail \= request\.form\.get\(\'size_avail\'\)\n        color_avail \= request\.form\.get\(\'color_avail\'\)\n        description \= request\.form\.get\(\'description\'\)\n        is_hot \= request\.form\.get\(\'is_hot\'\)\n        is_new \= request\.form\.get\(\'is_new\'\)\n\n        new_shirt \= shirt\(shirt_name\, material_name\, producer_name\, price\, size_avail\, color_avail\, description\, is_hot\, is_new\)\n\n        db\.session\.add\(new_shirt\)\n        db\.session\.commit\(\)\;\n    except\:\n        return \&quot;fail\&quot;\n    else\:\n        return \&quot;complete\&quot;</find>
      <find>from preparation\.init import app\, db\, list_code\nfrom models\.info import info\, info_schema\nfrom flask import request\, jsonify\nimport random\n\nifs \= info_schema\(\)\nifss \= info_schema\(many \= True\)\n\n\@app\.route\(\&quot;\/info\&quot;\, methods\=\[\&quot;GET\&quot;\]\)\ndef get_all_info\(\)\:\n    try\:\n        all_info \= info\.query\.all\(\)\n        result \= ifss\.dump\(all_info\)\n        return jsonify\(\n            error_code \= 0\,\n            message \= \&quot;Query Execute Completely\.\&quot;\,\n            data \= result\.data\n        \)\n    except\:\n        return jsonify\(\n            error_code \= 1\,\n            message \= \&quot;Database Error\.\&quot;\n        \)\n\n\@app\.route\(\&quot;\/info\&quot;\, methods\=\[\&quot;POST\&quot;\]\)\ndef add_info\(\)\:\n    try\:\n        name \= request\.json\.get\(\'name\'\)\n        is_student \= request\.json\.get\(\'is_student\'\)\n        phone \= request\.json\.get\(\'phone\'\)\n        need \= request\.json\.get\(\'need\'\)\n\n        new_info \= info\(name\, is_student\, phone\, need\)\n\n        db\.session\.add\(new_info\)\n        db\.session\.commit\(\)\;\n        list_code\.append\(1\)\n\n        code \= str\(random\.randint\(1000\, 9999\)\)\n        while list_code\.index\(code\) \!\= \-1\:\n            code \= str\(random\.randint\(1000\, 9999\)\)\n    except\:\n        return jsonify\(\n            errorCode \= \&quot;2\&quot;\,\n            message \= \&quot;Adding failed\.\&quot;\n        \)\n    else\:\n\n        \#tien hanh gui den so nguoi dung thong qua tin nhan\n        return jsonify\(\n            error_code \= 0\,\n            message \= \&quot;Adding completed\.\&quot;\,\n            validate_code \= code\n\n        \)</find>
    </findStrings>
